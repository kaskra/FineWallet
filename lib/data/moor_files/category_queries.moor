import 'tables.moor';

_allCategories: SELECT * FROM categories;
_categoriesByType: SELECT * FROM categories WHERE isExpense=:isExpense;
_deleteCustomCategory: DELETE FROM categories WHERE id=:id AND NOT isPreset;

_allSubcategories: SELECT * FROM subcategories;
_allSubcategoriesOfCategory: SELECT * FROM subcategories WHERE categoryId=:categoryId;
